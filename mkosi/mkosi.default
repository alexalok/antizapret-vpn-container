# Antizapret VPN system container

[Distribution]
Distribution=debian
Release=buster

[Output]
#Format=directory
Format=tar
Output=rootfs.tar.xz

[Validation]
CheckSum=true
Sign=false

[Packages]
WithNetwork=true
Packages=bash
         python3-dnslib
         ipcalc
         sipcalc
         gawk
         idn
         iptables
         ferm
         openvpn
         knot-resolver
         inetutils-ping
         curl
         wget
         ca-certificates
         openssl
         host
         dnsutils
         bsdmainutils
         procps
         unattended-upgrades
         nano
         vim-tiny
         git
